    "@babel/preset-env": "^7.25.4",
    "@tailwindcss/postcss": "^4.1.6",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "browser-sync": "^3.0.3",
    "css-loader": "^7.1.2",
    "cssnano": "^7.0.7",
    "del": "^7.1.0",
    "gulp": "^5.0.0",
    "gulp-imagemin": "^9.1.0",
    "gulp-postcss": "^10.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "postcss": "^8.4.47",
    "postcss-easy-import": "^4.0.0",
    "webpack-stream": "^7.0.0"
  },
  "browserslist": [
    "cover 99.5% in BE, not dead, not op_mini all"
  ]
}
